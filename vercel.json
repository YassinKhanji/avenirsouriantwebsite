{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/index.mjs": {
      "runtime": "@vercel/node@3.1.0",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.mjs"
    }
  ],
  "env": {
    "NODE_VERSION": "20",
    "PORT": "3001"
  }
}
